<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="root" prefHeight="250.0" prefWidth="275.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Main">
   <children>
      <Label fx:id="descriptionLabel" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="222.0" text="Please sign in">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="usernameLabel" layoutX="14.0" layoutY="71.0" text="Username:" />
      <Label fx:id="passwordLabel" layoutX="14.0" layoutY="106.0" text="Password:" />
      <TextField fx:id="usernameTB" layoutX="110.0" layoutY="67.0" onKeyPressed="#checkKey" />
      <PasswordField fx:id="passwordTB" layoutX="110.0" layoutY="102.0" onKeyPressed="#checkKey"/>
      <Label fx:id="zoneLabel" layoutX="14.0" layoutY="41.0" prefHeight="17.0" prefWidth="222.0" />
      <Label fx:id="errorLabel" alignment="CENTER" layoutX="6.0" layoutY="181.0" prefHeight="63.0" prefWidth="262.0" textFill="RED" wrapText="true" />
      <Button fx:id="signInButton" layoutX="110.0" layoutY="156.0" mnemonicParsing="false" onAction="#signIn" text="Sign In" />
   </children>
</AnchorPane>
